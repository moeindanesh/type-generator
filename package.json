{
  "name": "@moeindana/type-generator",
  "version": "1.0.1",
  "description": "Generate TypeScript types from Swagger/OpenAPI JSON",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate": "bun src/cli.ts",
    "test": "bun test src/__tests__/*.test.ts",
    "prepare": "bun run build",
    "prepublishOnly": "bun test",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "files": [
    "dist",
    "src/templates"
  ],
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "types",
    "generator"
  ],
  "author": "moeindana",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "swagger-typescript-api": "^13.0.3"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5"
  },
  "bin": {
    "swagger-type-gen": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moeindana/type-generator.git"
  },
  "bugs": {
    "url": "https://github.com/moeindana/type-generator/issues"
  },
  "homepage": "https://github.com/moeindana/type-generator#readme",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
